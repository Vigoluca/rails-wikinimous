<%= form_with model: @article do |f| %>
  <div class="form-group">
    <%= f.label :title  %>
    <%= f.text_field :title, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :content %>
    <%= f.text_field :content, class: "form-control" %>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
<br>
<%= link_to "Back to your articles", articles_path %>
